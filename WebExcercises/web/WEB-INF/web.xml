<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>ex8.FirstServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>helloServlet</servlet-name>
        <servlet-class>ex8.HelloServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>visitorCount</servlet-name>
        <servlet-class>ex9.VisitorCount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>formHandler</servlet-name>
        <servlet-class>ex10.FormHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userAgent</servlet-name>
        <servlet-class>ex11.UserAgent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>visitorCountImg</servlet-name>
        <servlet-class>ex12.VisitorCountImg</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/firstservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>helloServlet</servlet-name>
        <url-pattern>/helloservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>visitorCount</servlet-name>
        <url-pattern>/visitorCount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>formHandler</servlet-name>
        <url-pattern>/formHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userAgent</servlet-name>
        <url-pattern>/userAgent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>visitorCountImg</servlet-name>
        <url-pattern>/visitorCountImage</url-pattern>
    </servlet-mapping>
</web-app>